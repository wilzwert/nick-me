# Below are default dev environment values
# If you need to change some values, please do not edit this file ; copy to .env.local and edit accordingly 
# See .env.e2e for local e2e testing values
VITE_API_BASE_URL=http://localhost:8081
VITE_ALTCHA_CHALLENGE_URL=http://localhost:8081/altcha
VITE_ALTCHA_TOKEN_EXPIRY_SECONDS=60
VITE_ALTCHA_TEST=false
MOCK_API=false

